{
  "singleQuote": false,
  "jsxSingleQuote": false,
  "tabWidth": 2,
  "objectWrap": "preserve",
  "trailingComma": "all",
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "useTabs": false,
  "endOfLine": "lf",
  "bracketSameLine": false,
  "overrides": [
    {
      "files": ["*.json", "*.yml", "*.yaml"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
